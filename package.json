{
  "name": "tmd-quasar",
  "version": "0.0.1",
  "description": "TMD Quasar App",
  "productName": "TMD Frontend based on Quasar",
  "author": "Daniel de Kay <daniel@dekay.org>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:api": "vitest --config vitest.config.api.ts run",
    "test:api:watch": "vitest --config vitest.config.api.ts",
    "dev": "quasar dev",
    "dev:prod-api": "cross-env APP_ENV=prod-api quasar dev",
    "build": "quasar build",
    "serve": "quasar serve dist/spa",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@quasar/extras": "^1.16.4",
    "axios": "^1.10.0",
    "graphql": "^16.11.0",
    "pinia": "^3.0.3",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@quasar/vite-plugin": "^1.9.0",
    "@types/node": "^20.5.9",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "timezone-mock": "^1.3.6",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
